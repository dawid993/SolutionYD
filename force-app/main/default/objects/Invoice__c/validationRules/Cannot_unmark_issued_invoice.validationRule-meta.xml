<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_unmark_issued_invoice</fullName>
    <active>true</active>
    <description>Can&apos;t unmark the issued invoice.</description>
    <errorConditionFormula>PRIORVALUE(IsIssued__c) &amp;&amp; !IsIssued__c</errorConditionFormula>
    <errorMessage>Invoice has already been issued.</errorMessage>
</ValidationRule>
